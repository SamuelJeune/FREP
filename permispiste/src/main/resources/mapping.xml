<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.permispiste.metier.ActionEntity" table="action" schema="permispiste">
        <id name="numaction" column="NUMACTION"/>
        <property name="actNumaction" column="ACT_NUMACTION"/>
        <property name="libaction" column="LIBACTION"/>
        <property name="scoremin" column="SCOREMIN"/>
    </class>
    <class name="com.permispiste.metier.AppartientEntity" table="appartient" schema="permispiste">
        <composite-id mapped="true" class="com.permispiste.metier.AppartientEntityPK">
            <key-property name="numjeu" column="NUMJEU"/>
            <key-property name="numaction" column="NUMACTION"/>
        </composite-id>
    </class>
    <class name="com.permispiste.metier.ApprenantEntity" table="apprenant" schema="permispiste">
        <id name="numapprenant" column="NUMAPPRENANT"/>
        <property name="nomapprenant" column="NOMAPPRENANT"/>
        <property name="prenomapprenant" column="PRENOMAPPRENANT"/>
    </class>
    <class name="com.permispiste.metier.EstAssocieEntity" table="est_associe" schema="permispiste">
        <composite-id mapped="true" class="com.permispiste.metier.EstAssocieEntityPK">
            <key-property name="numaction" column="NUMACTION"/>
            <key-property name="numobjectif" column="NUMOBJECTIF"/>
        </composite-id>
    </class>
    <class name="com.permispiste.metier.FixeEntity" table="fixe" schema="permispiste">
        <composite-id mapped="true" class="com.permispiste.metier.FixeEntityPK">
            <key-property name="nummission" column="NUMMISSION"/>
            <key-property name="numobjectif" column="NUMOBJECTIF"/>
        </composite-id>
    </class>
    <class name="com.permispiste.metier.IndicateurEntity" table="indicateur" schema="permispiste">
        <id name="numindic" column="NUMINDIC"/>
        <property name="numaction" column="NUMACTION"/>
        <property name="poids" column="POIDS"/>
    </class>
    <class name="com.permispiste.metier.InscriptionEntity" table="inscription" schema="permispiste">
        <composite-id mapped="true" class="com.permispiste.metier.InscriptionEntityPK">
            <key-property name="numapprenant" column="NUMAPPRENANT"/>
            <key-property name="numjeu" column="NUMJEU"/>
        </composite-id>
    </class>
    <class name="com.permispiste.metier.JeuEntity" table="jeu" schema="permispiste">
        <id name="numjeu" column="NUMJEU"/>
        <property name="libellejeu" column="LIBELLEJEU"/>
    </class>
    <class name="com.permispiste.metier.MissionEntity" table="mission" schema="permispiste">
        <id name="nummission" column="NUMMISSION"/>
        <property name="numjeu" column="NUMJEU"/>
        <property name="libmission" column="LIBMISSION"/>
    </class>
    <class name="com.permispiste.metier.ObjectifEntity" table="objectif" schema="permispiste">
        <id name="numobjectif" column="NUMOBJECTIF"/>
        <property name="libobectif" column="LIBOBECTIF"/>
    </class>
    <class name="com.permispiste.metier.ObtientEntity" table="obtient" schema="permispiste">
        <composite-id mapped="true" class="com.permispiste.metier.ObtientEntityPK">
            <key-property name="numapprenant" column="NUMAPPRENANT"/>
            <key-property name="numaction" column="NUMACTION"/>
        </composite-id>
        <property name="valeur" column="VALEUR"/>
    </class>
</hibernate-mapping>